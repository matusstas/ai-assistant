{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "AI SERVICE",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "module": "uvicorn",
            "console": "integratedTerminal",
            "args": [
                "ai_service.api.main:app",
                "--host",
                "0.0.0.0",
                "--port",
                "8081",
                "--reload"
            ],
            "env": {
                "PYTHONWARNINGS": "ignore"
            }
        },
        {
            "name": "API",
            "type": "debugpy",
            "request": "launch",
